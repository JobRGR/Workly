border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

.footer
  background-color rgb( 255, 255, 255 )
  width 100%
  height 100px

.footer_green_bg
  background-color rgb( 116, 174, 128 )
  width 100%
  height 40px
  padding-left 240px
  & img
      display inline-block
      vertical-align middle
  & p
      display inline-block
      font-size 16px
      font-family "OpenSans"
      color rgb( 255, 255, 255)
      margin 11px 0px

.footer_logo_bg
  display inline-block
  background-color rgb( 255, 255, 255 )
  margin -36px 0px 0px 120px
  width 92px
  height 92px
  border-radius 50%

.footer_nav-bar
  display inline-block
  vertical-align top
  margin 21px 18px
  & li
      display inline
      font-size 16px
      font-family "OpenSans"
      margin 0px 5px
      color rgb( 86, 86, 86 )

.footer_workly
  display inline-block
  vertical-align top
  margin 14px 120px 14px 0px
  float right
  font-size 18px
  font-family "Pacifico"
  color rgb( 86, 86, 86 )

.footer_green
  display inline-block
  color rgb( 116, 174, 128 )




